{
  "name": "@rcmade/hono-docs",
  "version": "1.0.23",
  "description": "Auto-generate OpenAPI 3.0 spec from Hono route types",
  "repository": {
    "type": "git",
    "url": "https://github.com/rcmade/hono-docs.git"
  },
  "bugs": {
    "url": "https://github.com/rcmade/hono-docs/issues"
  },
  "homepage": "https://github.com/rcmade/hono-docs#readme",
  "keywords": [
    "openapi",
    "hono",
    "typescript",
    "documentation",
    "cli",
    "generator"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "hono-docs": "./dist/cli/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    },
    "./cli": {
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts,.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "esbuild": "^0.25.5",
    "esbuild-register": "^3.6.0",
    "ts-morph": "^26.0.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.3",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.2.0",
    "husky": "^8.0.0",
    "openapi-types": "^12.1.3",
    "standard-version": "^9.5.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
