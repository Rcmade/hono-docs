{
  "name": "@rcmade/hono-docs",
  "version": "1.0.32",
  "description": "Auto-generate OpenAPI 3.0 spec from Hono route types",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/rcmade/hono-docs.git"
  },
  "bugs": {
    "url": "https://github.com/rcmade/hono-docs/issues"
  },
  "homepage": "https://github.com/rcmade/hono-docs#readme",
  "keywords": [
    "openapi",
    "hono",
    "typescript",
    "documentation",
    "cli",
    "generator"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "hono-docs": "dist/cli/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./core": "./dist/core/index.js",
    "./cli": "./dist/cli/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint . --ext .ts,.js",
    "prepare": "husky install"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "esbuild-register": "^3.6.0",
    "ts-morph": "^26.0.0",
    "tsx": "^4.21.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@types/node": "^24.10.13",
    "@types/yargs": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-format": "^1.4.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.5.0",
    "husky": "^8.0.3",
    "openapi-types": "^12.1.3",
    "standard-version": "^9.5.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
